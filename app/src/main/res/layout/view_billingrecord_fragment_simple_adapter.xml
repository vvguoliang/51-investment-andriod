<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/color_light_grey"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/transaction_linear1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/commont_dp10"
        android:background="@color/white"
        android:orientation="horizontal"
        android:visibility="visible">

        <TextView
            android:id="@+id/billing_bank1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="@dimen/commont_dp10"
            android:text="@string/app_name"
            android:textColor="@color/black_grey"
            android:textSize="@dimen/commont_dp14" />

        <TextView
            android:id="@+id/billing_bank5"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:padding="@dimen/commont_dp10"
            android:text="@string/app_name"
            android:textColor="@color/black_grey"
            android:textSize="@dimen/commont_dp14" />


        <TextView
            android:id="@+id/billing_name1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/commont_dp10"
            android:layout_weight="1.5"
            android:text="@string/app_name"
            android:textColor="@color/black"
            android:textSize="@dimen/commont_dp14" />

        <TextView
            android:id="@+id/billing_name4"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/commont_dp10"
            android:layout_weight="1"
            android:text="@string/app_name"
            android:textColor="@color/black"
            android:textSize="@dimen/commont_dp14" />


        <TextView
            android:id="@+id/billing_money1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="@dimen/commont_dp10"
            android:layout_weight="1"
            android:padding="@dimen/commont_dp10"
            android:text="@string/app_name"
            android:textColor="@color/color_light_green"
            android:textSize="@dimen/commont_dp14" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/transaction_linear2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/commont_dp10"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/commont_dp10">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/billing_name2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/commont_dp10"
                    android:text="@string/app_name"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commont_dp16" />

                <TextView
                    android:id="@+id/billing_bank2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/commont_dp10"
                    android:padding="@dimen/commont_dp10"
                    android:text="@string/app_name"
                    android:textColor="@color/black_grey"
                    android:textSize="@dimen/commont_dp14" />

            </LinearLayout>

            <TextView
                android:id="@+id/billing_money2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="@dimen/commont_dp10"
                android:padding="@dimen/commont_dp10"
                android:text="@string/app_name"
                android:textColor="@color/color_light_green"
                android:textSize="@dimen/commont_dp14" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/commont_dp05"
            android:background="@color/black_grey" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left|center"
            android:orientation="horizontal"
            android:padding="@dimen/commont_dp10">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/commont_dp10"
                android:src="@mipmap/ic_time" />

            <TextView
                android:id="@+id/billing_tiem2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/commont_dp10"
                android:text="@string/app_name"
                android:textColor="@color/black_grey"
                android:textSize="@dimen/commont_dp14" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:id="@+id/transaction_linear3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/commont_dp10"
        android:background="@color/white"
        android:orientation="vertical"
        android:visibility="gone">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:padding="@dimen/commont_dp10">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/billing_name3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/commont_dp10"
                    android:text="@string/app_name"
                    android:textColor="@color/black"
                    android:textSize="@dimen/commont_dp16" />

                <TextView
                    android:id="@+id/billing_bank3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginRight="@dimen/commont_dp10"
                    android:padding="@dimen/commont_dp10"
                    android:text="@string/app_name"
                    android:textColor="@color/black_grey"
                    android:textSize="@dimen/commont_dp14" />

            </LinearLayout>

            <TextView
                android:id="@+id/billing_money3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginRight="@dimen/commont_dp10"
                android:padding="@dimen/commont_dp10"
                android:text="@string/app_name"
                android:textColor="@color/color_pink_red"
                android:textSize="@dimen/commont_dp14" />

        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/commont_dp05"
            android:background="@color/black_grey" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="left|center"
            android:orientation="horizontal"
            android:padding="@dimen/commont_dp10">

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/commont_dp10"
                android:src="@mipmap/ic_time" />

            <TextView
                android:id="@+id/billing_tiem3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/commont_dp10"
                android:text="@string/app_name"
                android:textColor="@color/black_grey"
                android:textSize="@dimen/commont_dp14" />

        </LinearLayout>

    </LinearLayout>
</RelativeLayout>
